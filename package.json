{
  "name": "vite-acme-inventory",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "bun --bun react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "::db-push": "bun --bun drizzle-kit push",
    "::db-generate": "bun --bun  drizzle-kit generate",
    "::db-migrate": "bun --bun  drizzle-kit migrate",
    "::auth-generate": "bunx --bun @better-auth/cli generate --config ./app/libs/auth.server.ts --output ./app/database/schemas/auth.schema.ts --y",
    "::auth-seed": "bunx --bun @better-auth-kit/cli@latest seed --config ./app/libs/auth.server.ts",
    "generate": "bun ::auth-generate && bun ::db-generate && bun ::db-push && bun ::auth-seed"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@sinclair/typebox": "^0.34.37",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.2.12",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "isbot": "^5.1.27",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-router": "^7.5.3",
    "recharts": "^3.0.2",
    "remix-hook-form": "^7.1.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "use-dehydrated-state": "^0.1.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@better-auth-kit/seed": "^1.0.12",
    "@libsql/client": "^0.15.9",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.17",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}