{
  "name": "vite-acme-inventory",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "::db-push": "bun --bun drizzle-kit push",
    "::db-generate": "bun drizzle-kit generate",
    "::db-migrate": "bun drizzle-kit migrate",
    "::auth-generate": "bunx --bun @better-auth/cli generate --config ./app/libs/auth.server.ts --output ./app/database/schemas/auth.schema.ts --y"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@sinclair/typebox": "^0.34.37",
    "better-auth": "^1.2.12",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "isbot": "^5.1.27",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-router": "^7.5.3",
    "remix-hook-form": "^7.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@libsql/client": "^0.15.9",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.17",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}